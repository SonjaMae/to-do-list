{
  "name": "todolistapp",
  "version": "1.0.0",
  "description": "Simple to do list app using vanilla JavaScript",
  "directories": {
    "doc": "docs"
  },
  "default": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run static && npx parcel src/index.html",
    "static": "npx gulp",
