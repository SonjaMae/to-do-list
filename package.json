    "sass": "^1.40.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "ejs": "^3.1.6",
    "lodash": "^4.17.21",
    "redux": "^4.1.1",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonjaMae/to-do-list.git"
